@model List<SubjectMaster>
@{
    int i = 0;
    if(Model != null)
    {
        foreach (var item in Model)
        {
            i++;
    <tr data-item-id="@item.Id">
                    <td>@i</td>
                    <td>@item.ProgramName</td>
                    <td>@item.Branch</td>
                    <td>@item.Year</td>
                    <td>@item.SubjectName</td>
                    <td>@item.SubjectCode</td>
                    <td>@item.SubjectType</td>
                    <td>@item.TheoryMarks</td>
                    <td>@item.PracticalMarks</td>
                    <td>@item.PassingMarks</td>
                    <td>@item.EntryDate</td>
                    <td>
                        <div class="btn-group btn-group-sm" style="float: none;">
                            <a class="tabledit-edit-button btn btn-sm btn-success btn_edit" id="edit" data-animation="pulse" data-toggle="modal" data-target="#exampleModalLong-1" href="/Master/GetSubjectMaster_ById?Id=@item.Id" style="float: none; margin: 4px;"><span class="ti-pencil"></span></a>
                            <button class="tabledit-delete-button btn btn-sm btn-danger" style="float: none; margin: 4px;" onclick="DeleteSubject(SubjectId=@item.Id)"><span class="ti-trash"></span></button>
                        </div>
                    </td>
    </tr>
        }
    }
    else
    {
    <tr>
                <td colspan=8 class="text-center" style="color:red;">Record Not Found</td>
    </tr>
    }
}
