@{
    Layout = "_LayoutAdmin";
}
<div class="">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header bgred">
                    <div class="row">
                        <div class="col-sm-3">
                            <h4 style="color:black;">Exam Result</h4>
                        </div>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-sm-2">
                            <label>Exam Group</label>
                            <select class="form-control" id="ddlgroup">
                                <option value="0">:: Select Exam Group ::</option>
                            </select>
                        </div>
                        <div class="col-sm-2">
                            <label>Exam Type</label>
                            <select class="form-control" id="ddltype">
                                <option value="0">:: Select Exam Type ::</option>
                            </select>
                        </div>
                        <div class="col-sm-2">
                            <label>Program</label>
                            <select class="form-control" id="ddlprogram">
                                <option value="0">:: Select Program ::</option>
                            </select>
                        </div>
                        <div class="col-sm-2">
                            <label>Branch</label>
                            <select class="form-control" id="ddlbranch">
                                <option value="0">:: Select Branch ::</option>
                            </select>
                        </div>
                        <div class="col-sm-2">
                            <label>Year</label>
                            <select class="form-control" id="ddlyear">
                                <option value="0">:: Select Year ::</option>
                            </select>
                        </div>
                        <div class="col-sm-2"  style="padding-top: 27px;">
                            <button class="btn btn-outline-primary btn-sm">Save</button>
                        </div>
                    </div>
                    <div class="row mt-3">
                        <div class="col-sm-12">
                            <div class="table-responsive">
                                <table class="table table-striped" id="tblexamschedule">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>EnrollmentNo</th>
                                            <th>Student Name</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>