@model List<ExamGroupe>
@{
    int i = 0;
    foreach (var item in Model)
    {
        i++;
        <tr data-id="@item.Id">
            <td>@i</td>
            <td>@item.ExamGroupes</td>
            <td>0</td>
            <td>@item.ExamTypeName</td>
            <td>@item.Entrydate</td>
            <td>
                <div class="btn-group btn-group-sm" style="float: none;">
                    <button class="tabledit-edit-button btn btn-sm btn-success btn_edit" data-animation="pulse" data-toggle="modal" data-target="#exampleModalLong-1" onclick="EditExamGroup('@item.Id')" style="float: none; margin: 4px;"><span class="ti-pencil"></span></button>
                    <button class="tabledit-delete-button btn btn-sm btn-danger" style="float: none; margin: 4px;" onclick="DeleteExamGroup('@item.Id')"><span class="ti-trash"></span></button>
                </div>
            </td>
        </tr>
    }
}