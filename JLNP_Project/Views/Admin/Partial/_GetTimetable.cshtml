@model List<SubjectMaster>
@{
    int i = 0;
    if (Model.Count > 0)
    {
        foreach (var item in Model)
        {
            i++;
            <tr data-item-id="@item.Id">
                <td>@i</td>
                <td>@item.Days</td>
                <td>@item.Branch</td>
                <td>@item.Year</td>
                <td>@item.Period1</td>
                <td>@item.Period2</td>
                <td>@item.Period3</td>
                <td>@item.Period4</td>
                <td>@item.Period5</td>
                <td>@item.Period6</td>
            </tr>
        }
    }
    else
    {
        <tr>
            <td style="color:red;" colspan="10" class="text-center">Record Not Found</td>
        </tr>
    }
}