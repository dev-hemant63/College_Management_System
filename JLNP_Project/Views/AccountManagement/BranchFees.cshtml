<script src="~/js/jquery.min.js"></script>
<script>
$(document).ready(function() {
    loaddata();
});
    var loaddata = () => {
        $.post('/AccountManagement/BranchFees').done(function(result) {
            $('#tblbranchfees tbody').html(result);
        }).fail(function(xhr) {
            console.log(xhr.responseText)
        });
    }
var deletedata = (FeesId) => {
    $.post('/AccountManagement/DeleteFeesHead', {Id:FeesId}).done(function(result) {
            alert(result.msg);
        window.location.reload();
        }).fail(function(xhr) {
            console.log(xhr.responseText);
        });
}
</script>