        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header bgred">
                    <h4 style="color:white;">Branch Wise Fees</h4>
                </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-3">
                                <lable>From Date</lable>
                                <input type="date" class="form-control" placeholder="From Date">
                            </div>
                            <div class="col-md-3">
                                <lable>From Date</lable>
