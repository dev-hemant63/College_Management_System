@{
    Layout = "_layoutAdmin";
}
<div class="">
    <div class="">
        <!-- Page-Title -->
        @*<div class="row">
            <div class="col-sm-12">
                <div class="page-title-box">
                    
                </div>
            </div>
        </div>*@
        <!-- end page title end breadcrumb -->
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header bgred">
                            <h4 style="color:white;">Fees Submition</h4>
                        </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-3">
                                <lable>From Date</lable>
                                <input type="date" class="form-control" placeholder="From Date">
                            </div>
                            <div class="col-md-3">
                                <lable>From Date</lable>
                                <input type="date" class="form-control" placeholder="From Date">
                            </div>
                            <div class="col-md-3">
                                <lable>Student Name</lable>
                                <input type="text" class="form-control" placeholder="Student Name">
                            </div>
                            <div class="col-md-2">
                                <button type="button" onclick="getdata()" class="btn btn-outline-primary waves-effect waves-light" style="margin-top:20px;" id="btnsearch">Search</button>
                            </div>
                            <!-- end col -->
                        </div>
                        <div class="table-responsive" style="margin-top:12px;">
                            <table class="table table-bordered" id="tblstudent">
                                <thead class="bg-primary">
                                    <tr>
                                        <th>#</th>
                                        <th>Student Name</th>
                                        <th>Father Name</th>
                                        <th>Branch</th>
                                        <th>Year</th>
                                        <th>Amount</th>
                                        <th>Fees Submition Mode</th>
                                        <th>Submition Date</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/js/jquery.min.js"></script>
<script>
    $(document).ready(function() {
        Loaddata();
    });
    var Loaddata = function() {
        $.post('/AccountManagement/StudentSubmitFees').done(function(result) {
            $('#tblstudent tbody').html(result);
        }).fail(function(xhr) {
            console.log(xhr.responseText);
        });
    }
</script>