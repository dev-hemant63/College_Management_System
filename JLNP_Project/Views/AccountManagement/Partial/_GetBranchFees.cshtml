@model List<AccountManagement>
@{
    int i = 0;
    @if (Model.Count>0) {
        foreach (var item in Model)
        {
            i++;
            <tr data-Fees-Id="@item.Id">
                <td>@i</td>
                <td>@item.Branch</td>
                <td>@item.Year</td>
                <td>@item.Amount</td>
                <td>@item.EntryDate</td>
                <td>
                    <div class="btn-group btn-group-sm" style="float: none;">
                        <a href="/AccountManagement/GetFeesHeadById?Id=@item.Id" class="tabledit-edit-button btn btn-sm btn-success btn_edit" style="float: none; margin: 4px;"><span class="ti-pencil"></span></a>
                        <button href="" class="tabledit-delete-button btn btn-sm btn-danger" onclick="deletedata('@item.Id')" style="float: none; margin: 4px;"><span class="ti-trash"></span></button>
                    </div>
                </td>
            </tr>
        } 
    }
    else
    {
       <tr>
        <td style="color:red;" colspan="13" class="text-center">Record Not Found</td>
       </tr>
    }
}