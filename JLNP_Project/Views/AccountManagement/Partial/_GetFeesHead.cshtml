@model List<AccountManagement>
@{
    if (@Model.Count > 0)
    {
        int i = 0;
        foreach (var item in Model)
        {
            i++;
            <tr>
                <td>@i</td>
                <td>@item.ProgramName</td>
                <td>@item.Branch</td>
                <td>@item.Year</td>
                <td>@item.FeesTypeName</td>
                <td>@item.Amount</td>
                <td>@item.EntryDate</td>
                <td>
                    <div class="btn-group btn-group-sm" style="float: none;">
                        <a href="/AccountManagement/GetFeesHeadById?ID=@item.Id" class="tabledit-edit-button btn btn-sm btn-success btn_edit" style="float: none; margin: 4px;"><span class="fa fa-pencil"></span></a>
                        <button href="" class="tabledit-delete-button btn btn-sm btn-danger" onclick="deletedata('@item.Id')" style="float: none; margin: 4px;"><span class="fa fa-trash"></span></button>
                        </div>
                </td>
            </tr>
        }
    }
    else
    {

    }
}