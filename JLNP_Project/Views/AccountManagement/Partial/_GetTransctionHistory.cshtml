@model List<TransctionDetails>
@{
    if (@Model.Count > 0)
    {
        int i = 0;
        @foreach (var item in Model)
        {
            i++;
            <tr>
                <td>@i</td>
                <td>@item.Name [@item.EnrollmentNo]</td>
                <td>@item.Program [@item.Branch]</td>
                <td>@item.TotalFees</td>
                <td>@item.SubmitedFees</td>
                <td>@item.DueFees</td>
                <td>@item.EntryDate</td>
            </tr>
        }
    }
    else
    {
        <tr>
            <td class="text-danger text-center" onclose="7">No transaction found</td>
        </tr>
    }
}