@model List<DefineFeesStructureReqRes>
@{
    int i = 0;
    if (Model.Count > 0)
    {
        foreach (var item in Model)
        {
            i++;
            <tr>
                <td>@i</td>
                <td>@item.ProgramName</td>
                <td>@item.BranchName</td>
                <td>@item.Year</td>
                <td>@item.FeesType</td>
                <td>@item.EntryDate</td>
                <td>
                    <div class="btn-group btn-group-sm" style="float: none;">
                            <button class="tabledit-edit-button btn btn-sm btn-success btn_edit" data-animation="pulse" data-toggle="modal" data-target="#exampleModalLong-1" onclick="EditFeesType('@item.Id')" style="float: none; margin: 4px;"><span class="fa fa-pencil"></span></button>
                            <button class="tabledit-delete-button btn btn-sm btn-danger" style="float: none; margin: 4px;" onclick="DeleteFeesType('@item.Id')"><span class="fa fa-trash"></span></button>
                        </div>
                </td>
            </tr>
        }
    }
    else
    {
        <tr>
            <td class="text-danger text-center" colspan="7"></td>
        </tr>
    }
}