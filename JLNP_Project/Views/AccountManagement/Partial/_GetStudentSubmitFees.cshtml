@model List<AccountManagement>
@{
    int i = 0;
    @if (Model.Count>0) {
        foreach (var item in Model)
        {
            i++;
            <tr data-Fees-Id="@item.Id">
                <td>@i</td>
                <td>@item.StudentName</td>
                <td>@item.FatherName</td>
                <td>@item.Branch</td>
                <td>@item.Year</td>
                <td>@item.Amount</td>
                <td>@item.FeesSubmitionMode</td>
                <td>@item.EntryDate</td>
            </tr>
        } 
    }
    else
    {
       <tr>
        <td style="color:red;" colspan="13" class="text-center">Record Not Found</td>
       </tr>
    }
}