@model List<AssignmentModel>
@{
    if (@Model.Count > 0)
    {
        int i = 0;
        foreach (var item in Model)
        {
            i++;
            <tr>
                <td>@i</td>
                <td>@item.Subject</td>
                <td>@item.Assignment</td>
                <td>
                    <a href="#">TakeAssignment</a>
                </td>
            </tr>
        }
    }
}